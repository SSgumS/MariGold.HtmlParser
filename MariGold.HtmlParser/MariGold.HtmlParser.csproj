<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.2.0</Version>
    <Company>MariGold</Company>
    <Product>MariGold Framework</Product>
    <Description>A utility to convert html document into a tree of HtmlNode elements. It can also parse the css styles and apply to the html elements</Description>
    <Copyright>Copyright © MariGold 2015</Copyright>
    <PackageProjectUrl>https://github.com/SSgumS/MariGold.HtmlParser</PackageProjectUrl>
    <PackageTags>MariGold Html Parser</PackageTags>
    <PackageReleaseNotes>Bug fixes</PackageReleaseNotes>
	<PackageId>MariGold.HtmlParser.RtL</PackageId>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>MariGold</Authors>
    <AssemblyVersion>1.2.0.0</AssemblyVersion>
    <FileVersion>1.2.0.0</FileVersion>
	<RepositoryUrl>https://github.com/SSgumS/MariGold.HtmlParser</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Net.Http" Version="4.3.3" />
  </ItemGroup>

</Project>
